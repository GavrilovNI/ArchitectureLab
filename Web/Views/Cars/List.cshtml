@using Web.Data.Models
@model SomeModel

@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

@{
	foreach(var car in Model.cars) {
		<div>
			<h3>@car.Name</h3>
			<p>@car.Category.Name</p>
		</div>
	}
	@Html.ActionLink("ActionLink", "Test", "Cars", new { id = 14 })
	<a class="btn btn-success" asp-controller="Cars" asp-action="Test2" asp-route-id="14" asp-route-idx="16">Кнопка с текстом</a>

	<form asp-action="TestPost" method="post" class="form-horizontal">
		<div class="form-group">
			<label asp-for="Name"></label>
			<div class="col-md-5">
				<input asp-for="Name" class="form-control" />
				<span asp-validation-for="Name" class="text-danger"></span>
			</div>
			<input class="btn btn-success" type="submit" value="Submit" />
		</div>
	</form>
			
}